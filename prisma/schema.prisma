generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FPLPlayer {
  player_id Int @unique @id
  name      String
  team      Int
  web_name   String
  team_code  Int
  element_type Int
}